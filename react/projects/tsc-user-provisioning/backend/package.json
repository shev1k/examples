{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "type": "commonjs",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "setup:prod": "npm i; cp .env.example .env",
    "setup:dev": "npm i; cp .env.example .env",
    "start:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register ./src/index.ts",
    "start:dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --cls --respawn ./src/index.ts",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "migrator": "ts-node ./scripts/migrator.ts",
    "migrate:up": "npm run migrator -- up",
    "migrate:down": "npm run migrator -- down",
    "migrate:create": "npm run migrator -- create --name",
    "seeder": "ts-node --transpile-only ./scripts/seeder.ts",
    "seed:all": "npm run seeder -- --all",
    "seed:one": "npm run seeder -- --name"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-guard": "^1.0.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.8.0",
    "ramda": "^0.28.0",
    "sequelize": "^6.25.0",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.4",
    "@types/nodemailer": "^6.4.6",
    "@types/ramda": "^0.28.15",
    "@types/sequelize": "^4.28.14",
    "@types/umzug": "^2.3.3",
    "@types/uuid": "^8.3.4",
    "commander": "^9.4.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "umzug": "^3.2.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
